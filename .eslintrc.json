{
    "extends": ["airbnb", "prettier", "react-app", "eslint:recommended", "plugin:react/recommended"],
    "plugins": ["react", "prettier"], 
    "rules": {
      "prettier/prettier": "error",
      "no-unused-vars": "warn",
      "no-console": "off",
      "func-names": "off",
      "react/prop-types": "off",
      "react/jsx-props-no-spreading": "off"
    }
}